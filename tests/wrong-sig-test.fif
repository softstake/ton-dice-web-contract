// Test #3. Throw exception when wrong signature provided.

"test-env.fif" include

"../keys/not-owner.pk" load-generate-keypair
=: not-owner-private-key
drop

variable signature

0 =: next-id

now timeout + 32 << <b x{11111111} s, b> hashu 32 1<<1- and + =: last-cleaned

dictnew constant init-temp-dict
<b b> <s last-cleaned init-temp-dict 64 udict! drop
<b swap dict, last-cleaned 64 u, owner_public_key B, init-queue-dict dict, 0 Gram, next-id 32 u, b> storage !

<b last-cleaned 1+ 64 u, x{11111111} s, b> message-to-sign !
message-to-sign @ hashu not-owner-private-key ed25519_sign_uint signature !
<b signature @ B, message-to-sign @ ref, b> <s message-to-send !

message-to-send @
recv_external
contract-code <s
storage @
c7
runvmctx

variable exit_code

drop
exit_code !
."Exit code " exit_code @ . cr
exit_code @ 34 <> abort"Test #3 Not passed"